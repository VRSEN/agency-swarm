# Agency endpoint auth for /openclaw-poc/* routes
APP_TOKEN=

# OpenClaw runtime settings
OPENCLAW_AUTOSTART=true
OPENCLAW_HOST=127.0.0.1
OPENCLAW_PORT=18789
OPENCLAW_GATEWAY_TOKEN=openclaw-local-token
OPENCLAW_DEFAULT_MODEL=openai/gpt-4o-mini
OPENAI_API_KEY=

# Optional override if `openclaw` binary is missing
# OPENCLAW_GATEWAY_COMMAND=npx -y openclaw@latest gateway --verbose
# Optional explicit path to repo root .env used to source OPENAI_API_KEY
# OPENCLAW_REPO_ENV_PATH=/absolute/path/to/.env

# Optional local proxy override. By default the app derives this from PORT.
# OPENCLAW_PROXY_BASE_URL=http://127.0.0.1:8080/openclaw/v1
OPENCLAW_PROXY_API_KEY=sk-openclaw-proxy
OPENCLAW_AGENT_MODEL=openclaw:main

# Persistent storage mount expected on Agencii
MNT_DIR=/mnt
OPENCLAW_DATA_DIR=/mnt/openclaw
OPENCLAW_STATE_DIR=/mnt/openclaw/state
OPENCLAW_CONFIG_PATH=/mnt/openclaw/openclaw.json
OPENCLAW_LOG_PATH=/mnt/openclaw/openclaw-gateway.log
